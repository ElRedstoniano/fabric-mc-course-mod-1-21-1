{
	"schemaVersion": 1,
	"id": "mccourse",
	"version": "${version}",
	"name": "MC Course Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"El_Redstoniano"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mccourse/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.kaupenjoe.mccourse.MCCourseMod"
		],
		"mm:early_risers": [
			"net.kaupenjoe.mccourse.fabricasm.EarlyRiser"
		],
		"fabric-datagen": [
			"net.kaupenjoe.mccourse.MCCourseModDataGenerator"
		],
		"client": [
			"net.kaupenjoe.mccourse.MCCourseModClient"
		],
		"terrablender": [
			"net.kaupenjoe.mccourse.MCCourseMod"
		],
		"rei_client": [
			"net.kaupenjoe.mccourse.compat.MCCourseModREIClient"
		],
		"rei_common": [
			"net.kaupenjoe.mccourse.compat.MCCourseModREICommon"
		]
	},
	"mixins": [
		"mc-course-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}